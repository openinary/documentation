{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Openinary Docs",
  "colors": {
    "primary": "#000000",
    "light": "#FFFFFF"
  },
  "favicon": "/favicon.svg",
  "navigation": {
